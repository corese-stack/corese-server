    "Institut National de la Recherche Agronomique" 
    "ITER Organisation"  
    "Institut de pharmacologie moléculaire et cellulaire"
    )
.  


st:continentList
    # parameter name in service URL: &mode=value
    st:name "mode" ; 
    # parameter name in table: st:get(st:mode) = value
    st:uri st:mode ;
