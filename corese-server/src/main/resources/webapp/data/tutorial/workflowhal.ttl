   ?x foaf:name "Olivier Corby" ;
    ?p ?y 
    ?z ?q ?x   
}
where {
service <https://data.archives-ouvertes.fr/sparql> {
    ?x foaf:name "Olivier Corby" ;
    ?p ?y 
    ?z ?q ?x 
}

